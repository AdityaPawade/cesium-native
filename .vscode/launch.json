    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test",
            "preLaunchTask": "Debug",
            "program": "${workspaceFolder}/build-debug/CesiumNativeTests/cesium-native-tests",
            "args": [],
            "environment": [],
            "cwd": "${workspaceFolder}",
            "type": "cppdbg",
            "request": "launch",
            "console": "integratedTerminal",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "text": "set print elements 0"
                }
            ]
        },
        {
            "name": "npm run generate-tilers-config",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run-script", "generate-tilers-config"]
        },
        {
            "name": "npm run generate-gltf",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run-script", "generate-gltf"]
        }
    ]
}